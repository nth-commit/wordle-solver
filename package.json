    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "cross-env": "^7.0.3",
    "fast-check": "^2.20.0",
    "gh-pages": "^3.2.3",
    "jest": "^27.4.7",
    "parcel": "^2.1.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
